.list-unstyled
  list-style none
  padding: 0
  
a.link:before
  content ''
  position absolute
  left 0
  right 0
  top 0
  bottom 0
  z-index 2

.pull-left
  float: left
.pull-right
  float: right
.text-left
  text-align left
.text-right
  text-align right
.text-uppercase
  text-transform: uppercase
.clearfix:after
  content: ''
  display: table
  clear: both
